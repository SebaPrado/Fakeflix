{
  "version": 2,
  "builds": [
    { "src": "/src/main.jsx", "use": "@vercel/node" },
    { "src": "/public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/main.jsx"
    }
  ]
}

// /src.main.jsx
