{
  "version": 2,
  "builds": [
    { "src": "/src/main.jsx", "use": "@vercel/node" },
    { "src": "/src/public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/src/css/(.*)",
      "dest": "/src/App.css"
    },
    {
      "src": "/(.*)",
      "dest": "/src/main.jsx"
    }
  ]
}
